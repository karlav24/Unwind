    <View
        android:id="@+id/sad_emoji"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="248dp"
        android:layout_marginTop="359dp"
        android:background="@drawable/sad_emoji"
        android:elevation="4dp"/>

    <View
        android:id="@+id/happy_emoji"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="107dp"
        android:layout_marginTop="359dp"
        android:background="@drawable/happy_emoji"
        android:elevation="4dp"/>

    <TextView
        android:id="@+id/how_are_you"
        android:layout_width="307dp"
        android:layout_height="58dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="316dp"
        android:gravity="top"
        android:text="@string/how_are_you"
        android:textAppearance="@style/how_are_you" />

    <View
        android:id="@+id/line_1"
        android:layout_width="281dp"
        android:layout_height="7dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="61dp"
        android:layout_marginTop="382dp"
