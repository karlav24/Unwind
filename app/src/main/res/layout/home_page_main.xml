        android:layout_width="match_parent"
        android:layout_height="125dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="18dp"
        android:background="@drawable/rectangle_home"
        android:elevation="4dp" />

    <FrameLayout
        android:id="@+id/fragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/rectangle_home"
        android:layout_alignBottom="@+id/rectangle_home"
        android:layout_alignStart="@+id/rectangle_home"
        android:layout_alignEnd="@+id/rectangle_home"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="18dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:elevation="6dp"/>


    <View
