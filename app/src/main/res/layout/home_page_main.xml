        android:layout_height="39dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="74dp"
        android:gravity="top"
        android:text=""
        android:textAppearance="@style/welcome_bac" />

    <View
