<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/android_lar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:clipToOutline="true" >


    <View
        android:id="@+id/back_button"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="27dp"
        android:background="@drawable/back_button" />

    <TextView
        android:id="@+id/settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="264dp"
        android:layout_marginTop="17dp"
        android:gravity="top"
        android:text="@string/settings"
        android:textAppearance="@style/settings" />

    <TextView
        android:id="@+id/account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="172dp"
        android:gravity="top"
        android:text="@string/account"
        android:textAppearance="@style/account" />


    <TextView
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="270dp"
        android:layout_marginLeft="279dp"
        android:layout_marginTop="678dp"
        android:gravity="top"
        android:text="@string/logout"
        android:textAppearance="@style/logout" />

    <View
        android:id="@+id/logout_button"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="220dp"
        android:layout_marginTop="687dp"
        android:background="@drawable/logout_button" />

    <TextView
        android:id="@+id/unwind_prem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:background="#FFFFFF"
        android:layout_marginTop="313dp"
        android:gravity="top"
        android:text="@string/unwind_prem"
        android:textAppearance="@style/unwind_prem" />

    <TextView
        android:id="@+id/privacy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="454dp"
        android:gravity="top"
        android:text="@string/privacy"
        android:textAppearance="@style/privacy" />


